{
  "name": "ms-fds",
  "version": "0.0.1",
  "description": "Micro service that handles FDS",
  "main": "dist/index.js",
  "private": true,
  "type": "module",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "------------------ BUILD -----------------": "",
    "build": "rm -rf ./dist && rm -f tsconfig.tsbuildinfo && npx tsc && npx tsc-alias",
    "------------------- CI --------------------": "",
    "check-types": "npx tsc",
    "check-circular": "madge --extensions js,ts --circular ./",
    "------------------- DB --------------------": "",
    "migrate": "FORCE_UNIQUE_TRANSACTION=true ./scripts/run-task.sh db.migrate --no-new-transaction",
    "migration:create": "create-migration --templatePath $(pwd)/node_modules/@padoa/database/resources/template.ts --migrationDirectory src/migrations",
    "init-db": "./scripts/run-task.sh db.init --no-new-transaction",
    "init-db:test": "NODE_ENV=test npm run init-db",
    "------------------ HUSKY ------------------": "",
    "prepare": "husky install",
    "pre-commit": "bash ./tools/is_main_branch.sh && lint-staged",
    "pre-push": "bash ./tools/is_main_branch.sh",
    "------------------ LINT ------------------": "",
    "lint": "eslint --fix --ext .ts --cache .",
    "lint:quiet": "eslint --fix --ext .ts --cache --quiet .",
    "lint:circleci": "eslint --quiet --ext .ts --format junit --output-file ./reports/eslint.xml --cache --config .eslintrc.ci.cjs .",
    "---------------- REPOSITORY ---------------": "",
    "generate-repository-files": "node node_modules/@padoa/database/dist/bin/repository-generator.js --ormPath @orm --topicsPath src/topics --topicsImportPath @topics --sequelizePath @helpers/database/index",
    "------------------- RUN ------------------": "",
    "start:compiled": "node index.js",
    "start:dev": "NODE_ENV=local npm run start:watch",
    "start:debug": "SQL=true npm run start:dev",
    "start:e2e": "NODE_ENV=web_e2e npm run start:watch",
    "start:watch": "tsx watch --inspect=9230 src/index.ts | pino-pretty -i hostname -t",
    "----------------- SWAGGER -----------------": "",
    "create-swagger": "node --async-stack-traces ./create-swagger.js",
    "create-swagger:dev": "tsx ./src/create-swagger.ts",
    "------------------ TASKS -----------------": "",
    "extract-data-from-fds": "LOG_LEVEL=error tsx ./src/tasks/extract/from-fds.ts",
    "extract-data-from-all-fds": "LOG_LEVEL=error tsx ./src/tasks/extract/from-all-fds.ts",
    "------------------ TEST ------------------": "",
    "test": "NODE_ENV=test npm run init-db && npx vitest run --no-threads --no-isolate --coverage",
    "test-update": "NODE_ENV=test npx vitest --update",
    "jest:circleci": "LOG_LEVEL=error npx vitest run --no-threads --no-isolate --coverage --reporter junit --outputFile reports/test-results.xml",
    "------------------ YARN ------------------": "",
    "yarn": "yarn install --frozen-lockfile",
    "yarn:retry": "bash ./ci-tools/scripts/retry_command.sh 'npm run yarn'",
    "yarn:production": "yarn workspaces focus --production",
    "yarn:retry:production": "bash ./ci-tools/scripts/retry_command.sh 'npm run yarn:production'"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/padoa/ms-fds.git"
  },
  "keywords": [
    "padoa",
    "ms",
    "chemical-risk",
    "fds"
  ],
  "author": "padoa",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/padoa/ms-fds/issues"
  },
  "homepage": "https://github.com/padoa/ms-fds#readme",
  "dependencies": {
    "@padoa/async-local-storage": "^2.0.2",
    "@padoa/axios": "^5.0.4",
    "@padoa/chemical-risk": "^1.3.0",
    "@padoa/csv-tools": "^2.0.4",
    "@padoa/database": "^26.2.0",
    "@padoa/express": "^15.0.2",
    "@padoa/file-system": "5.4.0-77c5a1d74b70a8e127b30f31714dfc8b1374838b",
    "@padoa/joi": "^5.0.2",
    "@padoa/logger": "^3.0.2",
    "@padoa/meta": "5.1.0-77c5a1d74b70a8e127b30f31714dfc8b1374838b",
    "@padoa/promise": "^3.0.3",
    "@padoa/sanitizer": "^1.2.6",
    "@padoa/spawn-director": "^4.0.4",
    "@padoa/swagger": "^15.0.3",
    "@padoa/tasks": "^4.0.3",
    "@padoa/time": "^2.0.3",
    "@types/yargs": "^17.0.31",
    "config": "^3.3.9",
    "date-fns": "^2.30.0",
    "express": "^4.18.2",
    "html-entities": "^2.4.0",
    "lodash": "^4.17.21",
    "pdf2json": "^3.0.5",
    "pdf2pic": "^3.0.3",
    "sequelize": "^6.33.0",
    "tesseract.js": "^5.0.3",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@padoa/eslint-plugin": "^2.0.11",
    "@padoa/prettier": "^2.0.2",
    "@types/config": "^3.3.2",
    "@types/express": "^4.17.20",
    "@types/lodash": "^4.14.200",
    "@types/multer": "^1.4.9",
    "@types/node": "^20.8.9",
    "@types/pino": "^7.0.5",
    "@types/supertest": "^2.0.15",
    "@vitest/coverage-v8": "^0.34.6",
    "husky": "^8.0.3",
    "lint-staged": "^10.5.3",
    "madge": "^6.0.0",
    "nodemon": "^3.0.1",
    "pino-pretty": "^10.2.3",
    "supertest": "^6.3.3",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
  "packageManager": "yarn@3.6.4"
}
